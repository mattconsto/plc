/*
  Problem 10: Delayed feedback
  - First 2 outputs same, then an added to value output at time n - 2
*/

int delayCount = 0;
int last = 0;
int beforeLast = 0;

loop {
  int value = console.read_int;

  if (delayCount < 2) then {
    delayCount = delayCount + 1;
    console.println_int value;
  } else {
    value = value + beforeLast;
    console.println_int value;
  } done;

  beforeLast = last;
  last = value;
}
